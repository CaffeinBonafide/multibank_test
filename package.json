{
  "name": "multibank-cypress-automation",
  "version": "1.0.0",
  "description": "Professional Cypress automation framework for MultiBank trading platform",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:headless": "cypress run --headless",
    "cy:run:parallel": "cypress run --record --parallel",
    "test": "npm run cy:run",
    "test:chrome": "npm run cy:run:chrome",
    "test:firefox": "npm run cy:run:firefox",
    "test:edge": "npm run cy:run:edge",
    "test:all-browsers": "npm run test:chrome && npm run test:firefox && npm run test:edge",
    "charfreq": "node scripts/charfreq.js",
    "charfreq:test": "node scripts/charfreq.test.js",
    "report:generate": "cypress-multi-reporters",
    "lint": "eslint cypress --ext .js,.ts",
    "lint:fix": "eslint cypress --ext .js,.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf cypress/screenshots cypress/videos reports",
    "pretest": "npm run clean && npm run type-check",
    "posttest": "npm run report:generate"
  },
  "keywords": [
    "cypress",
    "automation",
    "testing",
    "multibank",
    "trading",
    "e2e",
    "typescript"
  ],
  "author": "QA Automation Team",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0",
    "typescript": "^5.3.0",
    "@cypress/grep": "^4.0.1",
    "@cypress/xpath": "^2.0.3",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-mochawesome-reporter": "^3.8.0",
    "cypress-real-events": "^1.11.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-axe": "^1.5.0",
    "cypress-failed-log": "^2.10.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "rimraf": "^5.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaffeinBonafide/multibank_test.git"
  },
  "bugs": {
    "url": "https://github.com/CaffeinBonafide/multibank_test/issues"
  },
  "homepage": "https://github.com/CaffeinBonafide/multibank_test#readme"
}